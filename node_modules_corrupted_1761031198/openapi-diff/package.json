    "envsub": "^4.1.0",
    "express": "^4.19.2",
    "gulp": "^5.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-filter": "^7.0.0",
    "gulp-git": "^2.11.0",
    "gulp-jasmine": "^4.0.0",
    "gulp-tslint": "^8.1.4",
    "gulp-typescript": "^5.0.1",
    "jasmine": "^3.99.0",
