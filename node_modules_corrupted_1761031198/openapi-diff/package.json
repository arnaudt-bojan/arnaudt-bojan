    "@types/jasmine": "^3.10.18",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.14.112",
    "@types/node": "^14.18.63",
    "@types/request": "^2.48.12",
    "@types/swagger-schema-official": "^2.0.25",
    "@types/verror": "^1.10.10",
    "ansi-colors": "^4.1.3",
